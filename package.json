{
  "name": "immer-deps",
  "version": "0.0.2",
  "description": "Auto update depenencies, when immer's produce.",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "typings": "lib/esm/index.d.ts",
  "scripts": {
    "test": "jest --config jest.config.json",
    "build": "run-p build:*",
    "build:cjs": "tsc --outDir lib/cjs -p tsconfig.json",
    "build:mjs": "tsc --outDir lib/mjs -p tsconfig.json -m esnext",
    "release": "np --any-branch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hrsh7th/immer-deps.git"
  },
  "keywords": [
    "immer",
    "dependency",
    "graph",
    "tree"
  ],
  "author": "hrsh7th",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hrsh7th/immer-deps/issues"
  },
  "homepage": "https://github.com/hrsh7th/immer-deps#readme",
  "peerDependencies": {
    "immer": "^1.12.1"
  },
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "@types/uuid": "^3.4.4",
    "immer": "^1.12.1",
    "jest": "^23.6.0",
    "np": "^4.0.2",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.4"
  },
  "dependencies": {
    "uuid": "^3.3.2"
  }
}
